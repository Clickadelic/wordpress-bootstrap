{
	"name": "wordpress-bootstrap",
	"version": "1.1.0",
	"description": "A classic WordPress-Theme based on Bootstrap 5+.",
	"main": "index.js",
	"scripts": {
		"style-css": "sass ./assets/scss/frontend/style.scss ./assets/css/style.css --no-source-map --watch",
		"backend-css": "sass ./assets/scss/backend/bootstrap-backend.scss ./assets/css/bootstrap-backend.css --no-source-map --watch"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Clickadelic/wordpress-bootstrap.git"
	},
	"keywords": [
		"WordPress",
		"Bootstrap"
	],
	"author": "Tobias Hopp",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/Clickadelic/wordpress-bootstrap/issues"
	},
	"homepage": "https://github.com/Clickadelic/wordpress-bootstrap#readme",
	"dependencies": {
		"bootstrap": "^5.3.1"
	},
	"devDependencies": {
		"sass": "^1.64.1"
	}
}